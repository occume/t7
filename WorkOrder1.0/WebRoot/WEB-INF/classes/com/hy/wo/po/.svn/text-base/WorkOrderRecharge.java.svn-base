package com.hy.wo.po;

import java.util.Date;

/**
 * 工单充值信息
 * @author dong_jin
 *
 */
public class WorkOrderRecharge {
	private long id;
	private WorkOrder workOrder;//所属工单
	private String card;
	private String phoneCard;
	private Date rechargeTime;//充值时间
	private String rechargeType;//充值类型
	private int sum;
	private String rechargeErrorType;
	
	public int getSum() {
		return sum;
	}
	public void setSum(int sum) {
		this.sum = sum;
	}
	public String getRechargeErrorType() {
		return rechargeErrorType;
	}
	public void setRechargeErrorType(String rechargeErrorType) {
		this.rechargeErrorType = rechargeErrorType;
	}
	public String getCard() {
		return card;
	}
	public void setCard(String card) {
		this.card = card;
	}
	public String getPhoneCard() {
		return phoneCard;
	}
	public void setPhoneCard(String phoneCard) {
		this.phoneCard = phoneCard;
	}
	public WorkOrder getWorkOrder() {
		return workOrder;
	}
	public void setWorkOrder(WorkOrder workOrder) {
		this.workOrder = workOrder;
	}
	
	public long getId() {
		return id;
	}
	public void setId(long id) {
		this.id = id;
	}
	public Date getRechargeTime() {
		return rechargeTime;
	}
	public void setRechargeTime(Date rechargeTime) {
		this.rechargeTime = rechargeTime;
	}
	public String getRechargeType() {
		return rechargeType;
	}
	public void setRechargeType(String rechargeType) {
		this.rechargeType = rechargeType;
	}
	
}
