*{margin:0;padding:0}
html{overflow-x:hidden;}
#lookAll{cursor:pointer;}
#lookMore{cursor:pointer;}
/*****************通知样式*********************/
#outWall{position:absolute;top:0;width:100%;background-color:#fcc;display:none;}
#closeAll{margin:0 0 6px 6px;font-size:15px;cursor:pointer;}
#noticeWall{position:relative;overflow:hidden;zoom:1;margin:0 auto;}
.pack{position:relative;float:left;}
.notice{overflow:hidden;zoom:1;width:350px;min-height:150px;margin:6px;border:1px solid #E1E1E3;
	padding:8px;
	border-radius:10px;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	box-shadow:2px 3px 5px #d3d3d3;
	-moz-box-shadow:2px 3px 5px #d3d3d3;
	-webkit-box-shadow:2px 3px 5px #d3d3d3;
	filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#fefefe, endColorstr=#e0e0e2);
	background: linear-gradient(top, #fefefe, #f6f6f6 ,#f3f3f3,#f2f2f2,#e0e0e2);
	background: -moz-linear-gradient(top, #fefefe, #f6f6f6 ,#f3f3f3,#f2f2f2,#e0e0e2);
	background: -webkit-gradient(linear, 0 0, 0 100% , from(#fefefe),to(#e0e0e2));
	background: -webkit-linear-gradient(0 0, #fefefe, #f6f6f6 ,#f3f3f3,#f2f2f2,#e0e0e2);
	
	FONT-FAMILY: Tahoma;
}
.notice .title{color:#0099cc;height:18px;line-height:18px;padding:5px;border-bottom:1px dashed #0099cc;}
.notice .title .t1{width:65%;float:left;font-size:16px;padding-left:65px;}
.notice .title .close{width:12%;float:left;font-size:13px;color:#666666;cursor:pointer;}
.notice .content{clear:both;min-height:80px;font-size:15px;color:#666666;padding:5px;}
.notice .tail{text-align:right;margin-top:3px;}
.notice .tail img{vertical-align:middle;}

/***********************派单通知样式**************************/
#assignWall{position:absolute;top:0;width:100%;background-color:#9cc;display:none;}
#assignWall div{}
#assignWall .apack{display:none;position:relative;overflow:hidden;border:2px solid black;width:80%;zoom:1;margin:5px auto;background-color: #fff;cursor:pointer;}
#assignWall .anotice{font-size: 25px;padding:15px;}
#assignWall .anotice .content{color:#699;display: inline;}
#assignWall .anotice .tail img{vertical-align:middle;}
#assignWall .anotice .tail{}
#assignWall #closeAssign{margin:0 0 6px 6px;font-size:15px;cursor:pointer;}
/***********************工单窗样式*************************/
#woview {position:absolute;display:none;top:1px;left:150px; cursor:pointer;font-size:13px; FLOAT: right; OVERFLOW: hidden; COLOR: #fff; LINE-HEIGHT: 25px;HEIGHT: 25px;margin:25px 10px 0 0;}

