/************* FAQ WALL ***************/
#faq_wall{position:absolute;width:600px;height:700px;background:rgba(0, 0, 0, 0.6) none repeat scroll 0 0 !important;/*实现FF背景透明，文字不透明*/
filter:Alpha(opacity=60);background:#333;/*实现IE背景透明*/}
#faq_wall_tog{position:relative;background:#fff;width:80px;float: left;z-index: 999;}
#faq_wall_tog #tog_name{margin-bottom:6px;}
#faq_wall_con{position:relative;float: left;}
#faq_wall_con div{width:500px;}
#faq_wall_con .item .wrap{padding: 3px 0 0 5px;}
#faq_wall_con .item .d1{font-family:Tahoma;font-size: 16px;margin:5px 0 0 0;cursor:pointer;}
#faq_wall_con .item .d2{color:green;font-size: 13px;margin:5px 0 0 0;}
#faq_wall_con .item .d3{font-size: 14px;width:90%;padding:5px 30px 0 5px;}
#faq_wall_con .item .d4{font-size: 14px;width:90%;padding:5px 30px 0 5px;border-top:1px dashed #ccc;}
#faq_wall_con .item .d4 .rep{width:90%;border-bottom:1px dashed #ccc;}
#faq_wall_con .item .d4 #ind{font-family:Georgia;margin-right:5px;}
#faq_wall_con .item .d4 .pfoot{font-family:Georgia;margin-right:5px;color:#BC8F8F;}

#seek_box{padding:7px 0 7px 20px;}
#seek_box #seek_inp{padding:8px 5px;font:bold 16px red;width:300px;}
#seek_box #seek_send{padding:10px 5px 9px 5px;font:bold 16px red;width:100px;border:none;margin-left:5px;}

